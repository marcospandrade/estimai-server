{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach to Process",
            "type": "node",
            "request": "attach",
            "port": 9229,
            "skipFiles": ["<node_internals>/**/*.js", "server/node_modules/**/*.js"]
        },
        {
            "type": "chrome",
            "request": "attach",
            "name": "Attach to Open Chrome Window",
            "port": 9222,
            "webRoot": "${workspaceFolder}/client"
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Chrome against localhost",
            "url": "http://localhost:4200",
            "webRoot": "${workspaceFolder}/client"
        }
    ]
}
